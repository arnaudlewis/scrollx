{
  "name": "scrollx",
  "version": "2.1.1",
  "description": "scroll animation engine",
  "main": "dist/scrollx.js",
  "scripts": {
    "start": "npm run build",
    "watch": "babel lib/ --out-dir dist/ --watch",
    "build": "babel ./lib/ -d ./dist/",
    "prebuild": "mkdir -p ./dist",
    "prepublish": "npm run prebuild; npm run build;",
    "watch": "npm run prebuild; nodemon --watch ./lib --exec \"npm run build\"",
    "lint": "eslint lib/**/*.js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaudlewis/scrollx.git"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^3.8.1",
    "watchify": "^3.7.0"
  },
  "keywords": [
    "scroll",
    "animation"
  ],
  "author": "Arnaud Lewis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arnaudlewis/scrollx/issues"
  },
  "homepage": "https://github.com/arnaudlewis/scrollx#readme"
}
