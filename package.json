{
  "name": "scrollx",
  "version": "1.3.0",
  "description": "scroll animation engine",
  "main": "dist/scrollx.js",
  "scripts": {
    "start": "npm run build",
    "build": "browserify -t [babelify] ./lib/scrollx.js -d > ./dist/scrollx.js",
    "prebuild": "mkdir -p ./dist",
    "prepublish": "npm run prebuild; npm run build;",
    "watch": "npm run prebuild; nodemon --watch ./lib --exec \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arnaudlewis/scrollx.git"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "watchify": "^3.7.0"
  },
  "keywords": [
    "scroll",
    "animation"
  ],
  "author": "Arnaud Lewis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arnaudlewis/scrollx/issues"
  },
  "homepage": "https://github.com/arnaudlewis/scrollx#readme"
}
